{% extends 'base.html.twig' %}

{% block title %}Adsense{% endblock %}

{% block body %}
    <h1>Adsense</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ adsense.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ adsense.name }}</td>
            </tr>
            <tr>
                <th>Region</th>
                <td>{{ adsense.region }}</td>
            </tr>
            <tr>
                <th>Codes</th>
                <td>{{ adsense.codes }}</td>
            </tr>
            <tr>
                <th>Enable</th>
                <td>{{ adsense.enable ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('adsense_index') }}">back to list</a>

    <a href="{{ path('adsense_edit', {'id': adsense.id}) }}">edit</a>

    {{ include('adsense/_delete_form.html.twig') }}
{% endblock %}
